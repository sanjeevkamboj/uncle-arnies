          },
          {
            "type" : "image_picker",
            "id" : "line-img",
            "label" : "Select Line Image"
          },

          {
            "type" : "text",
            "id" : "company",
            "label" : "Enter Sub Heading"
          },
          {
            "type" : "url",
            "id" : "url-one",
            "label" : "Enter Sub Heading"
          },
          {
            "type" : "text",
            "id" : "shipping",
            "label" : "Enter Sub Heading"
          },
          {
            "type" : "url",
            "id" : "url-two",
            "label" : "Enter Sub Heading"
          },
          {
            "type" : "text",
            "id" : "lab",
            "label" : "Enter Sub Heading"
          },
          {
            "type" : "url",
            "id" : "url-three",
            "label" : "Enter Sub Heading"
          },
          {
            "type" : "text",
            "id" : "faq",
            "label" : "Enter Sub Heading"
          },
          {
            "type" : "url",
            "id" : "url-four",
            "label" : "Enter Sub Heading"
          }
        ]
      },
      {
        "type": "social-icons",
        "name" : "social-icons",
        "settings" : [
          {
            "type" : "image_picker",
            "id" : "icons-img",
            "label" : "Select Social Icons Images"
          },
          {
            "type" : "url",
            "id" : "icons-url",
            "label" : "Enter Social Icons Images Url"
          }
        ]
      }
    ],
    "presets" : [
      {
        "name" : "Custom Footer Section"
      }
    ]
  }
